{
    "dataset_reader": {
        "type": "squad_reader",
        "tokenizer": {
            "type": "bert-drop",
            "pretrained_model": "bert-base-uncased"
        },
        "token_indexers": {
            "tokens": {
                "type": "bert-drop",
                "pretrained_model": "bert-base-uncased"
            }
        },
        "lazy": true
    },
    "iterator": {
        "type": "basic",
        "batch_size": 4
    },
    "validation_iterator": {
        "type": "basic",
        "batch_size": 4
    },
    "model": {
        "type": "augmented_bert",
        "bert_pretrained_model": "bert-base-uncased",
        "dropout_prob": 0.1
    },
    "train_data_path": "https://allennlp.s3.amazonaws.com/datasets/squad/squad-train-v1.1.json",
    "validation_data_path": "https://allennlp.s3.amazonaws.com/datasets/squad/squad-dev-v1.1.json",
    "trainer": {
        "cuda_device": 0,
        "num_serialized_models_to_keep": 8,
        "num_epochs": 10,
        "optimizer": {
            "type": "bert_adam",
            "lr": 1e-05
        },
        "patience": 10,
        "summary_interval": 100,
        "validation_metric": "+f1"
    }
}